{
  "schemaVersion": 1,
  "id": "armor_hud",
  "version": "${version}",
  "name": "Armor Hud",
  "description": "Armor Hud displays the durability of your armor in a simple and clean way.",
  "authors": [
    "SaolGhra",
    "lilrollieguy"
  ],
  "contact": {},
  "license": "MIT",
  "icon": "assets/armor_hud/logo/ArmorHUDLogo.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.saolghra.armor_hud.client.Armor_hudClient"
    ],
    "main": [
      "com.saolghra.armor_hud.Armor_hud"
    ],
    "modmenu": [
      "com.saolghra.armor_hud.client.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "armor_hud.mixins.json",
    {
      "config": "armor_hud.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
